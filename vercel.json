{
  "buildCommand": null,
  "outputDirectory": ".",
  "functions": {
    "api/webhook.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/pricing",
      "destination": "/pricing.html",
      "permanent": false
    },
    {
      "source": "/signup",
      "destination": "/auth/signup.html",
      "permanent": false
    },
    {
      "source": "/signin",
      "destination": "/auth/signin.html",
      "permanent": false
    },
    {
      "source": "/login",
      "destination": "/auth/signin.html",
      "permanent": false
    },
    {
      "source": "/auth/callback",
      "destination": "/auth/callback.html",
      "permanent": false
    }
  ]
}
